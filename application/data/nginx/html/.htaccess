# BEGIN CACHE(Browser Cache)
<ifModule mod_expires.c>
ExpiresActive On
ExpiresByType image/gif "access plus 14 days"
ExpiresByType image/jpeg "access plus 14 days"
ExpiresByType image/png "access plus 14 days"

#ExpiresByType text/html "modification plus 1 days"
ExpiresByType text/html "access plus 1 seconds"
ExpiresByType text/css "access plus 60 minutes"
ExpiresByType application/x-javascript "access plus 60 minutes"

ExpiresByType application/xhtml+xml "access plus 14 days"
ExpiresByType application/xml "access plus 30 minutes"
ExpiresByType application/rss+xml "access plus 30 minutes"
ExpiresByType application/x-shockwave-flash "access plus 1 years"
</ifModule>
# END CACHE
# http://httpd.apache.org/docs/2.2/ja/mod/mod_expires.html

<filesMatch ".(jpg|jpeg|png|gif|swf|js|css)$">
ExpiresActive On
ExpiresDefault "access plus 30 days"
</filesMatch>


#FileEtag None


# BEGIN BlackJack
<IfModule mod_rewrite.c>
RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)/(.*)/(.*)/index.html$ /special/blackjack/index.php?lang=$1&n=$2&epi=$3 [L]
</IfModule>
# END BlackJack



# BEGIN WPSuperCache
# END WPSuperCache


# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.php [L]
</IfModule>

# END WordPress